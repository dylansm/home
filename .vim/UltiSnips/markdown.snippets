priority -50

snippet img "Image with path"
![${1:alt-text}](${2:path-to-image}${3: "${4:title}"})$0
endsnippet

snippet imgid "Image with referenced path"
![${1:alt-text}][${2:id}]$0
endsnippet

snippet a "Link"
[${1:text}](${2:link}${3: "Title"})$0
endsnippet

snippet aid "Link with reference"
[${1:text}][${2:id}]
[$2]: ${3:link} ${4:title}$0
endsnippet
